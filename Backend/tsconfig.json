{
	"compilerOptions": {
		"target": "es5",
		"module": "commonjs",
		"resolveJsonModule": true,
		"outDir": "./dist",
		"rootDir": "./src",
		"strict": false,
		"esModuleInterop": true,
		"skipLibCheck": true /* Skip type checking all .d.ts files. */
	},
	"include": ["src/**/*.ts"],
	"exclude": ["node_modules"]
}
